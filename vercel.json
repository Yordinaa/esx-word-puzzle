{
  "version": 2,
  "builds": [
    { "src": "src/server/server.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build" },
    {"buildCommand": "npm run run dev && node src/server/server.js"}

  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "src/server/server.js" },
    { "src": "/(.*)", "dest": "/" }
  ]

}
